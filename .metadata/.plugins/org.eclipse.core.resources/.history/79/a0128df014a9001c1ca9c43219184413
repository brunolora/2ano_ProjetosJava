package br.com.fiap.jpa.dao.imp1;

import br.com.fiap.jpa.dao.HibernateGenericDao;
import br.com.fiap.jpa.entity.Aluno;

public class AlunoDAOImp1 extends HibernateGenericDao<Aluno, Long> {
	
	private static AlunoDAOImp1 instance;
	
	public AlunoDaoImp1() {
		super(Aluno.class);
	}
	
	public static AlunoDAOImp1 getInstance() {
		if (instance == null) {
			instance = new AlunoDAOImp1();
		}
		return instance;
	}

}
